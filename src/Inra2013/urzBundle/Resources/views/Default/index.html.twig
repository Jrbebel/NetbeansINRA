        {% for agents in user if agents.Fonction=='Laborantin(e)' %}
            <tr>
              <td> {{agents.Nom|upper}}</td>
            <td> {{agents.Prenom}}</td>
            <td> {{agents.Email}}</td>
            <td> {{agents.Telephone}}</td>
            </tr>
            
           
             {% endfor %}
        </table>  
</p>
{% endblock %}